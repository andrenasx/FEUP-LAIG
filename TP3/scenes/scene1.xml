<lsf>
    <initials>
        <reference length="2" />
        <root id="root" />
    </initials>
    <views default="Top">
        <perspective id="Top" near="0.1" far="300" angle="30">
            <from x="4" y="10" z="16" />
            <to x="4" y="0" z="4" />
        </perspective>
    </views>

    <illumination>
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <background r="0.1" g="0.7" b="1.0" a="1.0" />
    </illumination>


    <lights>
        <light id="topCenter">
            <enable value="1" />    <!-- enable/disable -->
            <position x="7" y="7" z="4" w="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />   <!-- ambient component -->
            <diffuse r="0.7" g="0.7" b="0.7" a="1.0" /> <!-- diffuse component -->
            <specular r="0.8" g="0.8" b="0.8" a="0.8" />    <!-- specular component -->
        </light>

        <light id="left">
            <enable value="0" />
            <position x="0" y="3" z="8" w="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="0.2" g="0.2" b="0.2" a="1.0" />
            <specular r="0.6" g="0.6" b="0.6" a="1.0" />
        </light>

        <light id="back">
            <enable value="0" />
            <position x="7" y="5" z="-3" w="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="0.4" g="0.4" b="0.4" a="1.0" />
            <specular r="0.8" g="0.8" b="0.8" a="1.0" />
        </light>
    </lights>

    <textures>
    </textures>

    <spritesheets>
    </spritesheets>

    <materials>
        <material id="white_diffuse_material">
            <shininess value="1" />
            <ambient r="1.0" g="1.0" b="1.0" a="1.0" />
            <diffuse r="0.7" g="0.7" b="0.7" a="1.0" />
            <specular r="0.3" g="0.3" b="0.3" a="1.0" />
            <emissive r="0.0" g="0.0" b="0.0" a="1.0" />
        </material>

        <material id="red_material">
            <shininess value="1" />
            <ambient r="0.3" g="0" b="0" a="1.0" />
            <diffuse r="1" g="0" b="0" a="1.0" />
            <specular r="0" g="0" b="0" a="1.0" />
            <emissive r="0.0" g="0.0" b="0.0" a="1.0" />
        </material>

        <material id="blue_material">
            <shininess value="1" />
            <ambient r="0" g="0" b="0.3" a="1.0" />
            <diffuse r="0" g="0" b="1" a="1.0" />
            <specular r="0" g="0" b="0" a="1.0" />
            <emissive r="0.0" g="0.0" b="0.0" a="1.0" />
        </material>
    </materials>

    <animations>
    </animations>

    <nodes>     
        <node id="root">
            <material id="white_diffuse_material" />

            <texture id="clear">
                <amplification afs="1.0" aft="1.0" />
            </texture>

            <transformations>
            </transformations>
            <descendants>
            </descendants>
        </node>
    </nodes>
</lsf>